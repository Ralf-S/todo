{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "post"
      ],
      "route": "todos/"
    },
    {
      "type": "cosmosDB",
      "direction": "out",
      "name": "doc",
      "databaseName": "%DB_NAME%",
      "collectionName": "%COLLECTION_NAME%",
      "connectionStringSetting": "DB_CSTR"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ]
}
